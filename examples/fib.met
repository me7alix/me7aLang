extern print_int()
extern print_char()

func print_frame() {
	for i := 0; i < 7; i=i+1 {
		ch := (i + 65:int):i8
		print_char(ch)
		print_char(' ')
	}

	print_char('\n')
}

func main(): int {
	print_frame()

	cnt := 10
	a: int = 1
	b: int = 2

	for i := 0; i < cnt; i=i+1 {
		print_int(a)

		l := b
		b = b + a
		a = l
	}

	print_frame()
	return 0
}
