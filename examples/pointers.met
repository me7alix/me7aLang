extern malloc malloc_int(cnt: i64): *int
extern free   free_int(ptr: *int)
extern        print_int(val: int)

func main(): int {
	nums := malloc_int(40:i64)

	for i: i64 = 0; i < 10; i=i+1 {
		*(nums + i) = (i * i):int
	}

	for i: i64 = 0; i < 10; i=i+1 {
		print_int(*(nums + i))
	}
	
	free_int(nums)
	return 0
}
